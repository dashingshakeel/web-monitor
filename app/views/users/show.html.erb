<%= link_to "LOG OUT", logout_path, method: :delete ,class: "btn btn-primary" , id: "button1"%>


  

<div class="left-supervisors">
<%= link_to "Add new", new_supervisor_path,class: "btn btn-primary" , id: "button2"%>
  <table class="table">
  <thead>
    <tr>
      <th scope="row">Name</th>
      <th scope="row">URL:</th>
      <th scope="row"></th>
    </tr>
  </thead>

  <tbody>
    <% @supervisors.each do |supervisor| %>
      <tr>
        <td><%= link_to supervisor.name, supervisor_path(supervisor) %></td>
        <td class="url"><%= supervisor.url %></td>
        <td><%= link_to 'x', supervisor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


</div>



