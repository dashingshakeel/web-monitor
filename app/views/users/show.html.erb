<%= link_to "LOG OUT", logout_path, method: :delete ,class: "btn btn-primary" , id: "button1"%>

<div class="left-supervisors">
  <h5><button class="btn btn-primary btn-bordered" id="button2">Add Supervisor</button></h5>
  <table>
  <thead>
    <tr>
      <th>Name</th>
      <th>URL:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supervisors.each do |supervisor| %>
      <tr>
          <td><%= link_to supervisor.name, supervisor %></td>
        <td><%= supervisor.url %></td>
    
        <td><%= link_to 'Edit', edit_supervisor_path(supervisor) %></td>
        <td><%= link_to 'Destroy', supervisor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


</div>